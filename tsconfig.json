{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/libs/*": [
        "./src/libs/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/fonts/*": [
        "./src/app/fonts/*"
      ]
    },
    "strictNullChecks": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/app/api/generate-image/route.ts",
    "src/app/api/config/route.ts",
    "src/app/api/[username]/image/route.js",
    "src/app/image/route.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
